<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.AddSheetmusicActivity">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/add_sheetmusic_left_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/add_sheetmusic_center_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.35" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/add_sheetmusic_right_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.9" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/add_sheetmusic_name_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.11" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/add_sheetmusic_author_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.18" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/add_sheetmusic_files1_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.26"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/add_sheetmusic_files2_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.33" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/add_sheetmusic_genre_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.4" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/add_sheetmusic_key_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.465" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/add_sheetmusic_instrument_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.53" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/add_sheetmusic_mp3_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.6" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/add_sheetmusic_notes_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.68" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/add_sheetmusic_tags_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.77" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/add_sheetmusic_add_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.92" />


    <TextView
        android:id="@+id/add_sheetmusic_name_question"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_name"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_name_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_left_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        />


    <TextView
        android:id="@+id/add_sheetmusic_author_question"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_author"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_author_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_left_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        />

    <TextView
        android:id="@+id/add_sheetmusic_Files_question"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_files"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_files2_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_left_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        />

    <TextView
        android:id="@+id/add_sheetmusic_genre_question"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_genre"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_genre_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_left_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        />

    <TextView
        android:id="@+id/add_sheetmusic_key_question"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_key"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_key_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_left_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        />

    <TextView
        android:id="@+id/add_sheetmusic_instrument_question"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_instrument"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_instrument_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_left_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        />

    <TextView
        android:id="@+id/add_sheetmusic_mp3_question"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_mp3"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_mp3_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_left_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        />

    <TextView
        android:id="@+id/add_sheetmusic_notes_question"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_notes"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_notes_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_left_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        />

    <TextView
        android:id="@+id/add_sheetmusic_tags_question"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_tags"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_tags_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_left_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        />






    <EditText
        android:id="@+id/add_sheetmusic_name_answer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:hint="@string/text_name"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_name_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_right_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        />

    <EditText
        android:id="@+id/add_sheetmusic_author_answer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:hint="@string/text_author"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_author_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_right_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        />


    <TextView
        android:id="@+id/add_sheetmusic_pdf_files"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/pdf"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_files1_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_right_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        />

    <TextView
        android:id="@+id/add_sheetmusic_jpg_files"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/image_jpg_png"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_files2_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_right_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        />



    <EditText
        android:id="@+id/add_sheetmusic_genre_answer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:hint="@string/text_genre"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_genre_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_right_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        />

    <EditText
        android:id="@+id/add_sheetmusic_key_answer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:minWidth="48dp"
        android:minHeight="48dp"
        android:hint="@string/text_key"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_key_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_right_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_center_guideline" />

    <EditText
        android:id="@+id/add_sheetmusic_instrument_answer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:hint="@string/text_instrument"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_instrument_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_right_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        />


    <TextView
        android:id="@+id/add_sheetmusic_mp3_answer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_mp3_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_right_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        />


    <EditText
        android:id="@+id/add_sheetmusic_notes_answer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:minHeight="48dp"
        android:hint="@string/text_notes"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_notes_guideline"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_right_guideline"
        app:layout_constraintHorizontal_bias="0.0"/>

    <TextView
        android:id="@+id/add_sheetmusic_tags_answer"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text=""

        android:scrollbars="vertical"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_add_guideline"
        app:layout_constraintTop_toBottomOf="@id/add_sheetmusic_notes_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_edit_tags_button"
        app:layout_constraintStart_toStartOf="@id/add_sheetmusic_center_guideline"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.2"
        />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/add_sheetmusic_add_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:contentDescription="@string/describe_add_button"
        app:srcCompat="@android:drawable/ic_menu_save"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_add_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_right_guideline" />

    <ImageButton
        android:id="@+id/add_sheetmusic_edit_pdf_button"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:background="?android:selectableItemBackground"
        android:contentDescription="@string/describe_edit_button"
        app:srcCompat="@drawable/ic_edit"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_files1_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_right_guideline"
        app:layout_constraintHorizontal_bias="0.0" />

    <ImageButton
        android:id="@+id/add_sheetmusic_edit_jpg_button"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:background="?android:selectableItemBackground"
        android:contentDescription="@string/describe_edit_button"
        app:srcCompat="@drawable/ic_edit"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_files2_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_right_guideline"
        app:layout_constraintHorizontal_bias="0.0" />

    <ImageButton
        android:id="@+id/add_sheetmusic_edit_mp3_button"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:background="?android:selectableItemBackground"
        android:contentDescription="@string/describe_edit_button"
        app:srcCompat="@drawable/ic_edit"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_mp3_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_right_guideline"
        app:layout_constraintHorizontal_bias="0.0" />

    <ImageButton
        android:id="@+id/add_sheetmusic_edit_tags_button"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:background="?android:selectableItemBackground"
        android:contentDescription="@string/describe_edit_button"
        app:srcCompat="@drawable/ic_edit"
        app:layout_constraintBottom_toBottomOf="@id/add_sheetmusic_tags_guideline"
        app:layout_constraintEnd_toStartOf="@id/add_sheetmusic_right_guideline"
        app:layout_constraintHorizontal_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>

